import java.util.Scanner;
//Поиск среднего арифместического для массива
public class rep4 {
public static void main(String[] args) {
Scanner in = new Scanner(System.in);
int n= in.nextInt();
int arr[]=new int[n];
double k=0,s=0;
for (int i=0;i<n;i++){
arr[i]=in.nextInt();
}
for (int a:arr){
k+=1;
s+=a;
}
System.out.println(s/k);
}
}
